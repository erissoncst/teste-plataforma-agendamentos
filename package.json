  },
  "homepage": "https://github.com/erissoncst/teste-plataforma-agendamentos#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  }
}
