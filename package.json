{
  "name": "teste-plataforma-agendamentos",
  "version": "1.0.0",
  "description": "Testes E2E para plataforma de agendamentos usando Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
